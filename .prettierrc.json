{
	"semi": true,
	"singleQuote": true,
	"arrowParens": "avoid",
	"bracketSameLine": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"jsxSingleQuote": true,
	"printWidth": 80,
	"quoteProps": "as-needed",
	"useTabs": true,
	"tabWidth": 2,
	"proseWrap": "preserve"
}
