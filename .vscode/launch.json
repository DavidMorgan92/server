{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Jest Tests",
			"type": "node",
			"request": "launch",
			"runtimeArgs": [
				"--inspect-brk",
				"${workspaceRoot}/node_modules/jest/bin/jest.js",
				"--runInBand",
				"--testTimeout=1000000000"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}
	]
}
